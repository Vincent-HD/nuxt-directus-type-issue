{
	"name": "directus-extension-you-find-it",
	"version": "1.0.0",
	"type": "module",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"dependencies": {
		"@directus/api": "12.1.3",
		"@directus/composables": "^10.1.6",
		"@directus/errors": "^0.2.1",
		"@directus/extensions": "^0.2.0",
		"@directus/sdk": "^13.0.2",
		"@types/sprintf-js": "^1.1.4",
		"file-api": "^0.10.4",
		"file-saver": "^2.0.5",
		"fs": "0.0.1-security",
		"fs.promises": "^0.1.2",
		"jszip": "^3.10.1",
		"sprintf-js": "^1.1.3",
		"ts-toolbelt": "^9.6.0",
		"xlsx": "^0.18.5",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.2.0",
		"@directus/types": "^11.0.3",
		"@types/express": "^4.17.21",
		"@types/file-saver": "^2.0.7",
		"@types/node": "^20.10.6",
		"typescript": "^5.3.3",
		"vue": "^3.4.5"
	},
	"directus:extension": {
		"host": "^10.2.0",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "export-import-module",
				"source": "src/module/index.ts"
			},
			{
				"type": "endpoint",
				"name": "export-import-endpoint",
				"source": "src/endpoint/index.ts"
			}
		]
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"prepare": "directus-extension link ../directus/extensions",
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"imports": {
		"#shared/*": "./src/shared/*.ts",
		"#types/*": "./src/types/*.ts"
	}
}